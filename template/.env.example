# Environment Variables Template
# Copy this file to .env and fill in your actual values
# IMPORTANT: Never commit .env file to git!

# Application Environment
APP_ENV=development
# APP_COMMAND=app

# Database Credentials (example for PostgreSQL)
# Uncomment and fill after installing vandor vpkg add storage/postgres
# POSTGRES_HOST=localhost
# POSTGRES_PORT=5432
# POSTGRES_USER=postgres
# POSTGRES_PASSWORD=changeme
# POSTGRES_DB=myapp
# POSTGRES_SSL_MODE=disable

# Redis Credentials (example for cache/redis)
# Uncomment after installing vandor vpkg add cache/redis
# REDIS_HOST=localhost
# REDIS_PORT=6379
# REDIS_PASSWORD=
# REDIS_DB=0

# API Keys and Secrets
# Add your API keys and secrets here
# OPENAI_API_KEY=sk-...
# STRIPE_API_KEY=sk_test_...
# JWT_SECRET=your-super-secret-key-change-this

# MinIO/S3 Credentials (example)
# Uncomment after installing vandor vpkg add storage/minio
# MINIO_ACCESS_KEY=minioadmin
# MINIO_SECRET_KEY=minioadmin

# Email Service (example)
# SMTP_HOST=smtp.gmail.com
# SMTP_PORT=587
# SMTP_USER=your-email@gmail.com
# SMTP_PASSWORD=your-app-password

# Third-party Services
# SENTRY_DSN=https://...@sentry.io/...
# DATADOG_API_KEY=...

# Feature Flags
# ENABLE_FEATURE_X=false
# ENABLE_DEBUG_MODE=true
